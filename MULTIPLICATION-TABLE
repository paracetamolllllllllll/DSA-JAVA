import java.util.*;

public class Main{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    
	    System.out.println("print number to multiply: ");
	    int mul = sc.nextInt();
	    
	    for (int i=1; i<=10; i++){
	        System.out.println(mul+ "x" + i + "="+ (mul * i));
	    }
	}
}
